{{#if resData}}
<div class="container">
    <div class="card card-body mb-3">
        {{resData.name}}
        <br />
        {{resData.nip}}
        <br />
        {{resData.workingAddress}}
        <br />
        {{resData.statusVat}}
    </div>
</div>
{{else}}
<div class="container">
    <div class="card card-body mb-3">
        {{resData}}
    </div>
</div>
{{/if}}



