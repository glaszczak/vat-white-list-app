<div class="container">
  <div class="card card-body mb-3">
    NAME:
    {{resData.name}}
    <br />
    NIP:
    {{resData.nip}}
    <br />
    ADDRESS:
    {{resData.workingAddress}}
    <br />
    STATUS:
    {{resData.statusVat}}
  </div>

  <form class="form-inline">
    <div class="form-group mx-sm-3 mb-2">
      <label for="nip" class="sr-only">Nip</label>
      <input type="text" class="form-control" id="nip" placeholder="NIP">
    </div>
    <button type="submit" class="btn btn-primary mb-2">Check NIP</button>
  </form>
</div>